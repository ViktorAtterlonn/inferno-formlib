{
  "name": "inferno-formlib",
  "version": "6.0.3",
  "description": "Form library for inferno",
  "main": "dist/cjs/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "lib/index.js",
  "esnext": "src",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"TODO: Write tests...\"",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "build-dev": "rm -rf ./dist && NODE_ENV=development babel src --out-dir lib --watch --source-maps",
    "build": "npm run build:clean && cross-env NODE_ENV=development npm run build:es && cross-env NODE_ENV=development npm run build:dist && cross-env NODE_ENV=production npm run build:dist:prod && cross-env NODE_ENV=production npm run build:legacy",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "babel src --out-dir lib --source-maps",
    "build:legacy": "babel src --out-dir dist",
    "build:dist": "rollup -c",
    "build:dist:prod": "rollup -c",
    "build-test": "rm -rf ./test/dist && NODE_ENV=test node_modules/.bin/webpack --mode=development --config ./test/browser/webpack.config.js",
    "build-test-dev": "rm -rf ./test/dist && NODE_ENV=test node_modules/.bin/webpack --mode=development --watch --config ./test/browser/webpack.config.js"
  },
  "keywords": [
    "inferno",
    "isomorphic-schema",
    "formlib"
  ],
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jhsware/inferno-formlib.git"
  },
  "peerDependencies": {
    "component-registry": "1.0.0-rc.7",
    "isomorphic-schema": "1.0.0-beta.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "axios": "^0.17.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-inferno": "6.0.0",
    "babel-preset-inferno-app": "^8.0.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^0.1.19",
    "component-registry": "1.0.0-rc.7",
    "create-inferno-app": "^7.0.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.2",
    "formidable": "^1.2.0",
    "inferno-clone-vnode": "^6.0.3",
    "inferno-compat": "^6.0.3",
    "inferno-create-class": "^6.0.3",
    "inferno-create-element": "^6.0.3",
    "inferno-devtools": "^6.0.3",
    "inferno-router": "^6.0.3",
    "inferno-test-utils": "^6.0.3",
    "isomorphic-schema": "1.0.0-beta.7",
    "jest": "^23.6.0",
    "jsdom": "^11.6.2",
    "moment": "^2.22.2",
    "prismjs": "^1.9.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^6.1.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "sinon": "^2.3.8",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "classnames": "^2.2.6",
    "inferno": "^6.0.3",
    "inferno-animation": "^6.0.1",
    "inferno-bootstrap": "^6.0.1",
    "inferno-extras": "^6.0.3",
    "inferno-popper": "^6.0.2",
    "lodash.tonumber": "^4.0.3",
    "safe-utils": "^0.2.1",
    "striptags": "^3.1.1"
  }
}
